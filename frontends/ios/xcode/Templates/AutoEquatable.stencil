{% for type in types.implementing.AutoEquatable %}
extension {{ type.name }}: Equatable {
    public static func ==(lhs: {{ type.name }}, rhs: {{ type.name }}) -> Bool {
        {% for var in type.variables %}
        guard lhs.{{ var.name }} == rhs.{{ var.name }} else { return false }
        {% endfor %}

        return true
    }
}
{% endfor %}
